<li id="<%= projects_form.id %>">



	<div class="panel-group" id="#accordion">
	      <h4 class="panel-title">
	        <a data-toggle="collapse"  data-parent="#accordion" href='#collapse<%= projects_form.id %>'>
	          Projeto: <%= projects_form.name %>
	        </a>
            <%= link_to " >>>", projects_form %>
	      </h4>
	    <div id='collapse<%= projects_form.id %>' class="panel-collapse collapse">

	        
			
			<% if projects_form.timetables.any? %>
			  <ol class="microposts">
			    <%= render :partial => 'historic_form', :collection => projects_form.timetables %>
			  </ol>
			  <%#= will_paginate projects_form.timetables.paginate(page: params[:page], :per_page => 3) %>
			<% end %>


	        
			
	      </div>
	</div>

</li>
