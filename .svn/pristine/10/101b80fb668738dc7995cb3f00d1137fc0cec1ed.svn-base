<div class="row">
    <div class="span6 offset3">
      <%= form_for(@projects) do |f| %>
        <%= render 'layouts/error_messages', :object => f.object %>



      <div class="field">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
      </div>


      <div class="actions">
        <%= f.submit "Comfirm", :class => "btn btn-large btn-primary" %>
      </div>

    <% end %>
  </div>
</div>